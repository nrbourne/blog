<h1>Listing articles</h1>
 <%= link_to 'New article', new_article_path %>
 <%= link_to 'Sign Up', new_user_path %>
<table>
  <tr>
    <th>Title</th>
    <th>Text</th>
    <th>Created On:</th>
  </tr>
 
  <% @articles.each do |article| %>
    <tr>
      <td><%= article.title %></td>
      <td><%= article.text %></td>
      <td><%= article.created_at %></td>
    
    </tr>
  <% end %>
</table>